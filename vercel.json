{
    "version": 2,
    "builds": [
        {
            "src": "index.js", 
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/products",
            "dest": "app.js" 
        },
        {
            "src": "/(.*)",
            "dest": "app.js" 
        }
    ]
}
